{
  "name": "speed-test-monitor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && prisma db push && next build",
    "build:docker": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:example": "tsx prisma/seed.example.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "setup": "powershell -ExecutionPolicy Bypass -File setup.ps1",
    "setup:speedtest": "powershell -Command \"if (Get-Command winget -ErrorAction SilentlyContinue) { winget install Ookla.Speedtest.CLI } else { Write-Host 'Please install Speedtest CLI manually from https://www.speedtest.net/apps/cli' }\"",
    "check:speedtest": "powershell -Command \"if (Get-Command speedtest -ErrorAction SilentlyContinue) { speedtest --version } else { Write-Host 'Speedtest CLI not found. Run: npm run setup:speedtest' }\"",
    "db:seed:prod": "NODE_ENV=production tsx prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@prisma/client": "^6.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "eventsource": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.514.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "node-cron": "^4.1.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.59"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.20.0",
    "typescript": "^5"
  }
}
